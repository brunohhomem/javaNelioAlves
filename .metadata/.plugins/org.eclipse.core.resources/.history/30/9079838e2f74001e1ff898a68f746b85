
public class ExpreMatematica {

	public static void main(String[] args) {
		// Expressoes matem√°ticas

		double A, B, C, x, y, z;
		
		x = 3.0;
		y = 4.0;
		z = -5.0;
		
		A = Math.sqrt(x);
		B = Math.sqrt(y);
		C = Math.sqrt(25.0);
		System.out.println();
		System.out.println();
		System.out.println();
		
		A = Math.pow(x,  y);
		B = Math.pow(x, 2.0);
		C = Math.pow(5.0, 2.0);
		System.out.println();
		System.out.println();
		System.out.println();
		
		A = Math.abs(y);
		B = Math.abs(z);
		System.out.println();
		System.out.println();
	}

}
